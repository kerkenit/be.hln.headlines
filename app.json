{
	"id": "be.hln.headlines",
	"name": {
		"nl": "HLN Headlines",
		"en": "HLN Headlines"
	},
	"version": "0.1.4",
	"compatibility": "0.x || 1.x",
	"author": {
		"name": "Marco van 't Klooster",
		"email": "info@marcovantklooster.nl",
		"website": "http://www.marcovantklooster.nl"
	},
	"flow": {
		"actions": [{
			"id": "readNews",
			"title": {
				"en": "Read the news",
				"nl": "Lees het nieuws"
			}
		}]
